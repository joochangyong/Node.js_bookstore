  <div class="container" style="width: 40%; margin-left:30%;">

    <form class="form-signin" method='POST'>
      <h2 class="form-signin-heading">도서정보를 입력하세요.</h2>

      <input type="BookName" id="inputBookName" class="form-control" placeholder="제목" required autofocus
        name='BookName'>
      <input type="publish" id="inputpublish" class="form-control" placeholder="출판사" required name='publish'>
      <input type="stock" id="inputstock" class="form-control" placeholder="저자" required autofocus name='stock'>
      <input type="BookInfo" id="inputBookInfo" class="form-control" placeholder="줄거리" required autofocus
        name='BookInfo'>
      <input type="price" id="inputprice" class="form-control" placeholder="가격" required autofocus name='price'>
      <input type="bookAmount" id="inputbookAmountt" class="form-control" placeholder="수량" required autofocus
        name='bookAmount'>
      <button class="btn btn-lg btn-primary btn-block" type="submit" style="color: white; background-color: #222222;" >등록</button>
    </form>

  </div>

